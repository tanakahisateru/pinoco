{
    "name": "pinoco/pinoco",
    "homepage": "https://github.com/tanakahisateru/pinoco",
    "description": "Web site development framework using PHP and (mainly) PHPTAL",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "microframework",
        "phptal"
    ],
    "authors": [
        {
            "name": "Hisateru Tanaka",
            "email": "tanakahisateru@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phptal/phptal": "^1.4.0",
        "smarty/smarty": "^3.1.45|^4.1.1",
        "twig/twig": "^1.38.0|^2.14.11|^3.3.8",
        "phpunit/phpunit": "^8.5.29"
    },
    "autoload": {
        "psr-0": {
            "Pinoco": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/"
        }
    }
}
